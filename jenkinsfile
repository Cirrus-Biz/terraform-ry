pipeline {
    agent any
    stages {
        stage('Terraform Init') {
            steps {
                sh 'terraform init' 
            }   
        }
        stage('Terraform Plan') {
            steps {
                withCredentials([file(credentailsId: 'ry-terraform-vars', variable: 'ry-tfvars')])
                sh 'cat $ry-tfvars'
            }
        }
    }
}